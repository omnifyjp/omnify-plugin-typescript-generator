{
  "name": "@famgia/omnify-typescript",
  "version": "0.0.151",
  "description": "TypeScript type definitions generator for Omnify schemas",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "plugin": [
        "./dist/plugin.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./plugin": {
      "import": {
        "types": "./dist/plugin.d.ts",
        "default": "./dist/plugin.js"
      },
      "require": {
        "types": "./dist/plugin.d.cts",
        "default": "./dist/plugin.cjs"
      }
    }
  },
  "files": [
    "dist",
    "scripts",
    "stubs"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "postinstall": "node scripts/postinstall.js"
  },
  "keywords": [
    "omnify",
    "typescript",
    "types",
    "generator",
    "schema"
  ],
  "author": "famgia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ecsol/omnify-ts.git",
    "directory": "packages/typescript-generator"
  },
  "dependencies": {
    "@famgia/omnify-core": "workspace:*",
    "@famgia/omnify-types": "workspace:*"
  },
  "peerDependencies": {
    "zod": "^3.25.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
